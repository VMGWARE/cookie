{
  "name": "ui",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "rspack serve --node-env=development",
    "start:prod-proxy": "cross-env PROXY_HOST=https://discuit.net rspack serve --node-env=development",
    "build": "bun -b run generate:yaml && rspack --node-env=development",
    "build:prod": "bun -b run generate:yaml && rspack --node-env=production",
    "build:local": "bun -b run generate:yaml && rspack --node-env=local",
    "watch": "rspack --watch --node-env=development",
    "lint": "eslint --fix . --ext .jsx --ext .js",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json,scss,html}' --config ./.prettierrc",
    "generate:yaml": "bun -b run ./scripts/generate-yaml.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rspack/cli": "^0.7.5",
    "@rspack/core": "^0.7.5",
    "@types/bun": "^1.1.6",
    "compression-webpack-plugin": "^9.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-loader": "^3.1.2",
    "prettier": "^2.8.8",
    "pulltorefreshjs": "^0.1.22",
    "sass": "^1.77.8",
    "sass-loader": "^12.6.0",
    "typescript": "^5.5.4",
    "yaml": "^2.5.0"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet-async": "^1.3.0",
    "react-intersection-observer": "^8.34.0",
    "react-markdown": "^8.0.7",
    "react-redux": "^7.2.9",
    "react-router-dom": "^5.3.4",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.2",
    "remark-gfm": "^3.0.1"
  }
}
